<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="SanityTestSuit" parallel="tests" thread-count="4">
    <listeners>
        <listener class-name="com.mgs.TestBase.RetryAnnotation"/>
        <listener class-name="com.mgs.Utils.TestListeners"/>
    </listeners>

    <test name="Web Tests" parallel="classes" thread-count="4">
        <classes>
            <class name="com.mgs.Tests.ApiTest.GroceriesApis"/>
            <class name="com.mgs.Tests.WebTests.ClusterHomeExplore"/>
        </classes>
    </test>
</suite>
