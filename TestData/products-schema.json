{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer"
      },
      "category": {
        "type": "string"
      },
      "name": {
        "type": "string"
      },
      "inStock": {
        "type": "boolean"
      }
    },
    "required": ["id", "category", "name", "inStock"]
  }
}
