{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "name": {
      "type": "Integer"
    },
    "job": {
      "type": "string"
    },
    "id": {
      "type": "string",
      "pattern": "^[0-9]+$"
    },
    "createdAt": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": ["name", "job", "id", "createdAt"]
}
